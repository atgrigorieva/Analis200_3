\par \pard\sl360\slmult1\fi0\li120\tx120\qj\ri0\sb120\sa120\itap0 \plain \f1\fs20\cf0 \u1042  \u1087 \u1088 \u1086 \u1075 \u1088 \u1072 \u1084 \u1084 \u1077  \u1087 \u1088 \u1077 \u1076 \u1091 \u1089 \u1084 \u1086 \u1090 \u1088 \u1077 \u1085 \u1086  \u1076 \u1074 \u1072  \u1090 \u1080 \u1087 \u1072  \u1075 \u1088 \u1072 \u1076 \u1091 \u1080 \u1088 \u1086 \u1074 \u1086 \u1095 \u1085 \u1099 \u1093  \u1091 \u1088 \u1072 \u1074 \u1085 \u1077 \u1085 \u1080 \u1081 : \u1079 \u1072 \u1074 \u1080 \u1089 \u1080 \u1084 \u1086 \u1089 \u1090 \u1100  \u1086 \u1087 \u1090 \u1080 \u1095 \u1077 \u1089 \u1082 \u1086 \u1081  \u1087 \u1083 \u1086 \u1090 \u1085 \u1086 \u1089 \u1090 \u1080  \u1086 \u1090  \u1082 \u1086 \u1085 \u1094 \u1077 \u1085 \u1090 \u1088 \u1072 \u1094 \u1080 \u1080  A(C), \u1082 \u1072 \u1082  \u1087 \u1088 \u1077 \u1076 \u1087 \u1080 \u1089 \u1099 \u1074 \u1072 \u1077 \u1090 \u1089 \u1103  \u1073 \u1086 \u1083 \u1100 \u1096 \u1080 \u1085 \u1089 \u1090 \u1074 \u1086 \u1084  \u1085 \u1086 \u1088 \u1084 \u1072 \u1090 \u1080 \u1074 \u1085 \u1099 \u1093  \u1076 \u1086 \u1082 \u1091 \u1084 \u1077 \u1085 \u1090 \u1086 \u1074 , \u1080  \u1079 \u1072 \u1074 \u1080 \u1089 \u1080 \u1084 \u1086 \u1089 \u1090 \u1100  \u1082 \u1086 \u1085 \u1094 \u1077 \u1085 \u1090 \u1088 \u1072 \u1094 \u1080 \u1080  \u1086 \u1090  \u1086 \u1087 \u1090 \u1080 \u1095 \u1077 \u1089 \u1082 \u1086 \u1081  \u1087 \u1083 \u1086 \u1090 \u1085 \u1086 \u1089 \u1090 \u1080  C(A), \u1082 \u1072 \u1082  \u1088 \u1077 \u1072 \u1083 \u1080 \u1079 \u1086 \u1074 \u1072 \u1085 \u1086  \u1074 \u1086  \u1074 \u1085 \u1091 \u1090 \u1088 \u1077 \u1085 \u1085 \u1077 \u1084  \u1087 \u1088 \u1086 \u1075 \u1088 \u1072 \u1084 \u1084 \u1085 \u1086 \u1084  \u1086 \u1073 \u1077 \u1089 \u1087 \u1077 \u1095 \u1077 \u1085 \u1080 \u1080  \u1089 \u1087 \u1077 \u1082 \u1090 \u1088 \u1086 \u1092 \u1086 \u1090 \u1086 \u1084 \u1077 \u1090 \u1088 \u1072 .
\par \pard\sl360\slmult1\fi0\li120\tx120\qj\ri0\sb120\sa120\itap0 \u1048 \u1084 \u1077 \u1077 \u1090 \u1089 \u1103  \u1090 \u1088 \u1080  \u1074 \u1080 \u1076 \u1072  \u1072 \u1087 \u1087 \u1088 \u1086 \u1082 \u1089 \u1080 \u1084 \u1072 \u1094 \u1080 \u1080  \u1075 \u1088 \u1072 \u1076 \u1091 \u1080 \u1088 \u1086 \u1074 \u1086 \u1095 \u1085 \u1086 \u1081  \u1079 \u1072 \u1074 \u1080 \u1089 \u1080 \u1084 \u1086 \u1089 \u1090 \u1080 : \u1083 \u1080 \u1085 \u1077 \u1081 \u1085 \u1072 \u1103 , \u1087 \u1088 \u1086 \u1093 \u1086 \u1076 \u1103 \u1097 \u1072 \u1103  \u1095 \u1077 \u1088 \u1077 \u1079  \u1085 \u1072 \u1095 \u1072 \u1083 \u1086  \u1082 \u1086 \u1086 \u1088 \u1076 \u1080 \u1085 \u1072 \u1090 , \u1083 \u1080 \u1085 \u1077 \u1081 \u1085 \u1072 \u1103  \u1080  \u1082 \u1074 \u1072 \u1076 \u1088 \u1072 \u1090 \u1080 \u1095 \u1085 \u1072 \u1103  (\u1087 \u1072 \u1088 \u1072 \u1073 \u1086 \u1083 \u1080 \u1095 \u1077 \u1089 \u1082 \u1072 \u1103 ).
